<hibernate-mapping package="org.unitime.banner.model">
	<class  abstract="true"
		name="Queue">

 		<cache include="non-lazy" usage="read-write"/>
 		
        <id
            name="uniqueId"
            column="UNIQUEID"
            type="java.lang.Long"
        >
           <generator class="org.unitime.commons.hibernate.id.UniqueIdGenerator">
                <param name="sequence">queue_seq</param>
            </generator>
        </id>
        
	  <property 
	     lazy="false" 
	     name="xml"
	     column="xml"
	     type="org.unitime.commons.hibernate.blob.XmlClobType"
	     not-null="true"/>
		
		<property
			name="status"
			column="status"
			type="java.lang.String"
			not-null="false"
			lazy="false"
			length="10"
		/>

  		<property
  		   name="postDate"
  		  column="postdate"
           lazy="false"
   		   type="java.sql.Timestamp"/>
 
   		<property
  		   name="processDate"
  		  column="processdate"
           lazy="false"
   		   type="java.sql.Timestamp"/>
   	
   	     <union-subclass
		 	name="QueueIn"
		 	table="integrationqueuein"
		 	abstract="false">
		 	
			<property
			   name="matchId"
			   column="matchid"
			   type="java.lang.Long"
			   lazy="false"
     		/>	 	
		 	
   	     </union-subclass>	   
   	
   	     <union-subclass
		 	name="QueueOut"
		 	table="integrationqueueout"
		 	abstract="false">
		 	
   		  <property
  		     name="pickupDate"
  	 	     column="pickupdate"
             lazy="false"
   		     type="java.sql.Timestamp"/>
		 	
   	     </union-subclass>	   
   		   
    </class>
</hibernate-mapping>
