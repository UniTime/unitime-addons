<hibernate-mapping package="org.unitime.banner.model">
	<class
		name="BannerCampusOverride"
		table="banner_campus_override">

 		<cache include="non-lazy" usage="read-write"/>
 		
        <id
            name="uniqueId"
            column="uniqueid"
            type="java.lang.Long"
        >
            <generator class="org.unitime.commons.hibernate.id.UniqueIdGenerator">
                <param name="sequence">pref_group_seq</param>
            </generator>
        </id>
        
		<property
			name="bannerCampusCode"
			column="banner_campus_code"
			type="java.lang.String"
			not-null="true"
			length="20"
		/>
		
		<property
			name="bannerCampusName"
			column="banner_campus_name"
			type="java.lang.String"
			not-null="true"
			length="100"
		/>

		<property
			name="visible"
			column="visible"
			type="java.lang.Boolean"
			not-null="true"
		/>
		
	</class>

</hibernate-mapping>
